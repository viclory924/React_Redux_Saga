{
  "name": "frontegg",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "image-to-base64": "^2.0.1",
    "prismjs": "^1.24.0",
    "react": "^16.8.6",
    "react-collapsible": "^2.6.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.7",
    "react-files": "^2.4.8",
    "react-input-color": "^2.0.17",
    "react-ionicons": "^3.0.1",
    "react-prism": "^4.3.2",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-select-search": "^0.9.6",
    "react-switch": "^5.0.0",
    "react-tooltip": "^3.10.0",
    "recharts": "^1.6.2",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.3"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./src/index.scss --include-path ./node_modules src/ -o src/styles/css",
    "watch-css": "npm run build-css && node-sass --include-path ./src/index.scss --include-path ./node_modules src/ -o src/styles/css --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "start-js": "react-scripts start",
    "postbuild": "node iisConfig/copyIISConfig.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5"
  }
}
